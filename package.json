{
  "name": "themost-oauth2-server-src",
  "version": "0.1.1",
  "description": "OAuth2 server based on MOST Web Framework",
  "repository": "",
  "private": true,
  "author": "",
  "scripts": {
    "build": "babel ./modules/oauth2 --out-dir dist/modules/oauth2 --copy-files --ignore ./modules/oauth2/node_modules"
  },
  "dependencies": {
    "@themost/cli": "^2.1.9",
    "@themost/client": "^2.2.1",
    "@themost/node": "^2.2.1",
    "@themost/query": "^2.3.1",
    "@themost/xml": "^2.3.1",
    "@themost/common": "^2.1.5",
    "@themost/data": "^2.2.1",
    "@themost/mailer": "^2.1.1",
    "@themost/sqlite": "^2.1.2",
    "@themost/web": "^2.2.1",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-decorators": "^7.1.0",
    "@babel/plugin-transform-async-to-generator": "^7.1.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-transform-modules-commonjs": "^7.1.0",
    "@babel/plugin-transform-strict-mode": "^7.0.0",
    "@babel/plugin-transform-typeof-symbol": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "@types/lodash": "^4.14.116",
    "babel-eslint": "^10.0.1",
    "del": "^3.0.0",
    "eslint": "^5.6.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^8.0.0",
    "gulp-eslint": "^5.0.0"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "es6": true,
      "jquery": true
    },
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "after": false
    },
    "extends": "eslint:recommended",
    "rules": {
      "no-console": "off",
      "no-invalid-this": "warn",
      "no-undef": "error",
      "no-unused-vars": "warn",
      "no-var": [
        "error"
      ],
      "strict": [
        2,
        "never"
      ]
    }
  }
}
