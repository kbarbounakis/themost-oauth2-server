{
  "name": "themost-oauth2-server",
  "version": "0.1.1",
  "description": "OAuth2 server based on @themost/data ORM module",
  "main": "dist/server/server.js",
  "repository": "",
  "private": true,
  "author": "",
  "scripts": {
    "start": "node ./bin/www",
